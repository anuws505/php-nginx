# mysql run
docker run -d --name mysql56-run \
-v /Users/anuws505/php-nginx/mysql56:/var/lib/mysql \
-e MYSQL_ROOT_PASSWORD=password \
-p 3306:3306 mysql:5.6

# access mysql-app-run
docker exec -it mysql56-run /bin/bash