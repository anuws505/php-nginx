# debian:wheezy
docker container rm -f onlinestore-run
docker build -t onlinestore:v1.0 . --no-cache
docker run -d --name onlinestore-run -p 80:80 -p 443:443 onlinestore:v1.0

# access onlinestore-run
docker exec -it onlinestore-run /bin/bash

# or docker run
docker run -d --name onlinestore-run \
# -v certs:/opt/certs \
-v code:/var/www/html \
-p 80:80 -p 443:443 onlinestore:v1.0

# find . -type d -exec chmod 755 {} \;
# find . -type f -exec chmod 644 {} \;

